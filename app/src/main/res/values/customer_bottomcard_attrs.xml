<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomerBottomCard">
        <attr name="customer_behavior_peekHeight" format="dimension" />
        <attr name="customer_behavior_hideAble" format="boolean" />
        <attr name="customer_behavior_skipCollapse" format="dimension" />
        <attr name="customer_behavior_middleHeight" format="dimension" />
        <attr name="customer_behavior_fitToContents" format="boolean" />
        <attr name="customer_behavior_needBackGround" format="boolean" />
        <attr name="customer_behavior_defaultState" format="enum">
            <enum name="BIG" value="3" />
            <enum name="MIDDLE" value="6" />
            <enum name="SMALL" value="4" />
            <enum name="HIDE" value="5" />
        </attr>
    </declare-styleable>

</resources>